<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="445.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.operatingsystems.PageRemovalView">
   <children>
      <Label fx:id="authorLabel" layoutX="230.0" layoutY="423.0" text="Written and programmed by: Mia Watts">
         <font>
            <Font name="System Italic" size="11.0" />
         </font>
      </Label>
      <Button fx:id="fifoButton" layoutX="88.0" layoutY="81.0" mnemonicParsing="false" onAction="#onFIFOClick" text="First-In, First-Out (FIFO)" />
      <Button fx:id="lruButton" layoutX="418.0" layoutY="81.0" mnemonicParsing="false" onAction="#onLRUClick" text="Least Recently Used (LRU)" />
      <Label layoutX="230.0" layoutY="14.0" text="Page Removal Policies">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="32.0" layoutY="28.0" prefHeight="59.0" prefWidth="585.0" text="Click a button. The one on the left will show information related to the First-In, First-Out (FIFO) policy, and the second button will show information related to the Least Recently Used (LRU) policy." textAlignment="CENTER" wrapText="true" />
      <Label fx:id="answerLabel" alignment="TOP_CENTER" layoutX="32.0" layoutY="114.0" prefHeight="305.0" prefWidth="585.0" textAlignment="CENTER" wrapText="true" />
   </children>
</AnchorPane>
